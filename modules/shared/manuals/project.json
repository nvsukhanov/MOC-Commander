{
  "name": "shared-manuals",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/shared/manuals/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": ["scope:shared-feature"],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "modules/shared/manuals/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["modules/shared/manuals/**/*.ts", "modules/shared/manuals/**/*.html", "modules/shared/manuals/package.json"]
      }
    }
  }
}
